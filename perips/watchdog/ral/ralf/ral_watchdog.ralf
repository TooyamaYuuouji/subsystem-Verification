register WDOGLOAD {
    bytes 4;

    field LOAD_VALUE {
        bits 32;
        reset 32'hffffffff;
        access rw;
    }
}

register WDOGVALUE {
    bytes 4;

    field VALUE {
        bits 32;
        reset 32'hffffffff;
        access ro;
    }
}

register WDOGCONTROL {
    bytes 4;

    field CONTROL {
        bits 2;
        reset 2'b00;
        access rw;
    }
}

register WDOGINTCLR {
    bytes 4;

    field INT {
        bits 1;
        reset 1'b0;
        access wo;
    }
}

register WDOGRIS {
    bytes 4;

    field RIS {
        bits 1;
        reset 1'b0;
        access ro;
    }
}

register WDOGMIS {
    bytes 4;

    field MIS {
        bits 1;
        reset 1'b0;
        access ro;
    }
}

