block ral_watchdog {
    bytes 100;
    register WDOGLOAD @'h000;
    register WDOGVALUE @'h004;
    register WDOGCONTROL @'h008;
    register WDOGINTCLR @'h00C;
    register WDOGRIS @'h010;
    register WDOGMIS @'h014;
    register WDOGLOCK @'hC00;
    register WDOGITCR @'hF00;
    register WDOGITOP @'hF04;
    register WDOGPERIPHID4 @'hFD0;
    register WDOGPERIPHID5a @'hFD4;
    register WDOGPERIPHID6a @'hFD8;
    register WDOGPERIPHID7a @'hFDC;
    register WDOGPERIPHID0 @'hFE0;
    register WDOGPERIPHID1 @'hFE4;
    register WDOGPERIPHID2 @'hFE8;
    register WDOGPERIPHID3 @'hFEC;
    register WDOGPCELLID0 @'hFF0;
    register WDOGPCELLID1 @'hFF4;
    register WDOGPCELLID2 @'hFF8;
    register WDOGPCELLID3 @'hFFC;
    register WDOGRESERVE @'hFC0;
}